<div class="container mt-5">
  <div class="card gap-3 p-3 w-50">
  <h1>List of movie categories</h1>
  <ul class="list-group">
    <% @lists.each do |list|%>
      <li class="list-group-item">
        <%= list.id %>
        <%= link_to list.name, list_path(list) %>
      </li>
    <% end %>
  </ul>
  <%= link_to  "Create list", new_list_path, class: "btn btn-success" %>
  </div>
</div>
